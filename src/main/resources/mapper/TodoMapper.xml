<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.todostudy.todo.mapper.TodoMapper">

    <insert id="insertTodo" parameterType="com.todostudy.todo.domain.Todo"
            useGeneratedKeys="true" keyProperty="idx">
        INSERT INTO t_todo (userId, content, regDate, expDate, delYn)
        VALUES (#{userId}, #{content}, #{regDate}, #{expDate}, #{delYn})
    </insert>

</mapper>